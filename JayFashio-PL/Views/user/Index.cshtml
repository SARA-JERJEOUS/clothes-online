@model List<JayFashio_PL.Models.userlocationModel>

<table class="table table-hover table-bordered">

    <tr>
        <td>city</td>
        <td>Username </td>
        <td>Password</td>
        <td>Role</td>
        <td>Address1</td>
        <td>Address2</td>
        <td>FirstName</td>
        <td>LastName</td>
        <td>Email</td>
        <td>DateOFbirth</td>
        <td>Edit</td>





    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>@item.locuser </td>
            <td>@item.Username </td>
            <td>@item.Password </td>
            <td>@item.Role </td>
            <td>@item.Address1 </td>
            <td>@item.Address2 </td>
            <td>@item.FirstName </td>
            <td>@item.LastName </td>
            <td>@item.Email </td>
            <td>@item.DateOFbirth </td>
            <td>
                <button class="btn btn-sm  btn-success"> Edit </button>
            </td>
        </tr>

    }

</table>






