
@model List<JayFashio_M.Item>
@if (Model.Count == 0)
{
    <span>  no item</span>
}

@foreach (var item in Model)
{


    <div class="col-md-4">

        <div class="flip-card ">

            <div class="flip-card-inner ">
                <div class="flip-card-front  ">
                    <img src="@Url.Content(@item.PathItem)" style="width:200px;height:200px;" alt="Alternate Text" />
                </div>
            </div>
        </div>
    </div>
}